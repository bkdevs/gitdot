{
  "name": "@s2-dev/streamstore",
  "version": "0.22.5",
  "private": true,
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/s2-streamstore/s2-sdk-typescript"
  },
  "homepage": "https://s2.dev",
  "scripts": {
    "build": "rm -rf dist && npx tsx scripts/generate-version.ts && tsc --project tsconfig.build.esm.json && tsc --project tsconfig.build.cjs.json && npx tsx scripts/postbuild.ts && attw --pack",
    "test": "vitest --run",
    "check": "tsc --noEmit",
    "gen:version": "npx tsx scripts/generate-version.ts",
    "example:browser": "npx tsx examples/browser/serve.ts",
    "check:write": "biome check --write",
    "format": "biome format --write",
    "version-with-docs": "changeset version && bun run gen:version && bun run docs && git add docs",
    "release": "changeset publish",
    "gen:openapi": "openapi-ts",
    "gen:proto": "protoc --ts_out src/generated/proto --ts_opt ts_nocheck --proto_path s2-specs s2-specs/s2/v1/s2.proto && mv src/generated/proto/s2/v1/s2.ts src/generated/proto/s2.ts",
    "docs": "typedoc",
    "snippets": "node scripts/sync-snippets.mjs",
    "check:snippets": "tsc --noEmit -p examples/tsconfig.json"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@protobuf-ts/runtime": "^2.11.1",
    "debug": "^4.4.3"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "2.2.5",
    "@changesets/cli": "^2.29.7",
    "@hey-api/openapi-ts": "^0.86.12",
    "@protobuf-ts/plugin": "^2.11.1",
    "@types/bun": "^1.3.2",
    "@types/debug": "^4.1.12",
    "openapi-typescript": "^7.10.1",
    "protoc": "^33.1.0",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.14",
    "vitest": "^4.0.10"
  }
}
